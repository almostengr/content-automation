    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/netcoreapp3.1/smautomation.dll",
            "args": [
                // {
                // "wdTimeout": 15,
                // "youtubeUrl" : "https://www.youtube.com/channel/UC4HCouBLtXD1j1U_17aBqig",
                // "websiteUrl": "https://thealmostengineer.com"
                // }
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}